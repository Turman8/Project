{
    "files.hotExit": "off",
    "workbench.enableExperiments": false,
    "files.autoSave": "off",
    "editor.formatOnSave": false,
    "workbench.editor.restoreViewState": false,
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "none",
    "extensions.autoUpdate": false,
    "python.defaultInterpreterPath": "./venv/bin/python",
    "files.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/*.pyo": true,
        "**/*.log": true,
        "**/*.jou": true,
        "**/webtalk*": true,
        "**/*backup*": true,
        "**/Untitled-*": true,
        "**/*build*.tcl": true,
        "**/*deploy*.tcl": true,
        "**/*test*.tcl": true,
        "**/create_*.tcl": true,
        "**/*.ps1": true,
        "**/ecg_cnn_model.py": true,
        "**/ecg_feature_*.py": true,
        "**/ecg_filter.py": true,
        "**/ecg_main.py": true,
        "**/ecg_normalize.py": true,
        "**/ecg_preprocess.py": true,
        "**/ecg_segmenter.py": true,
        "**/ecg_visualizer.py": true,
        "**/ecg_wavelet_*.py": true,
        "**/extract_*.py": true,
        "**/signal_read.py": true,
        "**/test.py": true,
        "**/module_test.py": true,
        "**/*分析*.md": true,
        "**/CLEANUP_*.md": true
    },
    "search.exclude": {
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/*.pyo": true,
        "**/*.log": true,
        "**/*.jou": true,
        "**/webtalk*": true,
        "**/*backup*": true,
        "**/*.ps1": true,
        "**/*build*.tcl": true,
        "**/*deploy*.tcl": true,
        "**/*test*.tcl": true,
        "**/create_*.tcl": true
    },
    "files.watcherExclude": {
        "**/__pycache__/**": true,
        "**/*.pyc": true,
        "**/*.log": true,
        "**/*.jou": true,
        "**/webtalk*/**": true,
        "**/*backup*/**": true,
        "**/*.ps1": true,
        "**/*build*.tcl": true,
        "**/*deploy*.tcl": true,
        "**/*test*.tcl": true,
        "**/create_*.tcl": true
    }
}
